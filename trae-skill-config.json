{
  "name": "presearch-search-skill",
  "version": "1.0.0",
  "description": "Privacy-first decentralized web search for AI agents using Presearch API",
  "category": "search",
  "tags": ["search", "privacy", "decentralized", "ai-agents", "web-search", "autonomous-systems"],
  "author": {
    "name": "NosytLabs",
    "email": "collaborate@presearch.com",
    "url": "https://github.com/NosytLabs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NosytLabs/presearch-search-skill.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "python": ">=3.7"
  },
  "dependencies": {
    "python": ["requests>=2.25.0"],
    "node": []
  },
  "optionalDependencies": {
    "python": [
      "python-dotenv>=0.19.0",
      "aiohttp>=3.8.0",
      "pytest>=6.0.0",
      "black>=22.0.0",
      "flake8>=4.0.0"
    ]
  },
  "config": {
    "api_key_env": "PRESEARCH_API_KEY",
    "base_url": "https://na-us-1.presearch.com/v1/search",
    "rate_limit_delay": 1.1,
    "max_retries": 3,
    "timeout": 30
  },
  "examples": {
    "python": "python3 presearch_python.py 'latest AI developments'",
    "node": "node presearch_nodejs.js 'decentralized AI search'",
    "advanced": "python3 example.py"
  },
  "documentation": {
    "readme": "README.md",
    "skill": "SKILL.md",
    "license": "LICENSE"
  },
  "test": {
    "python": "python3 presearch_python.py 'test search'",
    "node": "node presearch_nodejs.js 'test search'"
  },
  "features": [
    "Privacy-first decentralized search",
    "Rate limiting and exponential backoff",
    "Location-based search",
    "Time-based filtering",
    "Pagination support",
    "Multi-language support",
    "Safe search options"
  ],
  "use_cases": [
    "Autonomous AI agents",
    "Local LLM integration",
    "Real-time information gathering",
    "Market research",
    "Fact-checking and verification",
    "Trend identification"
  ],
  "compatibility": {
    "frameworks": ["AutoGPT", "LangChain", "CrewAI"],
    "llms": ["Llama 2", "GPT-4", "Claude", "Mistral"],
    "orchestration": ["n8n", "Zapier", "Make.com"],
    "deployment": ["Docker", "Kubernetes", "serverless"]
  },
  "community": {
    "website": "https://presearch.com",
    "discord": "https://discord.gg/presearch",
    "telegram": "https://t.me/presearch",
    "twitter": "https://twitter.com/presearchnews",
    "support": "collaborate@presearch.com"
  }
}